{
  "name": "dag-service",
  "version": "0.0.0-dev",
  "description": "Library for storing and replicating hash-linked data over IPFS network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": {
    "./setup/node": "./setup/browser"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "polendina --cleanup src/**/*.spec.ts --webpack-config webpack.test.js",
    "test:node": "TS_NODE_FILES=true mocha -r ts-node/register -r source-map-support/register --recursive src/**/*.spec.ts --exit",
    "lint": "eslint --ext .mjs,.js,.jsx,.ts,.tsx src",
    "docs": "typedoc",
    "browser:build": "NODE_ENV=production webpack --mode production",
    "browser:serve": "npm run browser:build && http-server dist/browser --gzip",
    "browser": "npm run browser:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-dag-service.git"
  },
  "keywords": [
    "ipfs",
    "ipld",
    "dag-service",
    "dag",
    "typescript",
    "browser",
    "node"
  ],
  "author": "Carson Farmer <carson@textile.io>",
  "license": "(Apache-2.0 AND MIT)",
  "bugs": {
    "url": "https://github.com/ipfs/js-dag-service/issues"
  },
  "homepage": "https://github.com/ipfs/js-dag-service#readme",
  "devDependencies": {
    "@types/browser-or-node": "^1.3.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "browser-or-node": "^1.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "multicodec": "^2.1.0",
    "multihashes": "^3.1.0",
    "peer-info": "^0.17.5",
    "polendina": "^1.1.0",
    "prettier": "^2.2.1",
    "streaming-iterables": "^5.0.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@ipld/block": "^6.0.4",
    "@ipld/dag-cbor": "^2.0.3",
    "@ipld/dag-json": "^4.0.0",
    "@ipld/dag-pb": "0.0.1",
    "@types/interface-datastore": "^1.0.1",
    "bignumber.js": "^9.0.1",
    "buffer": "^6.0.3",
    "cids": "^1.1.4",
    "deepmerge": "^4.2.2",
    "interface-datastore": "^2.0.1",
    "ipfs-bitswap": "^4.0.0",
    "ipfs-core-types": "0.0.0",
    "ipfs-unixfs-exporter": "^3.0.7",
    "ipfs-unixfs-importer": "^5.0.0",
    "libp2p": "^0.29.3",
    "libp2p-bootstrap": "^0.12.1",
    "libp2p-crypto": "^0.18.0",
    "libp2p-gossipsub": "^0.6.1",
    "libp2p-kad-dht": "^0.20.4",
    "libp2p-mdns": "^0.15.0",
    "libp2p-mplex": "^0.10.1",
    "libp2p-noise": "^2.0.1",
    "libp2p-secio": "^0.13.1",
    "libp2p-tcp": "^0.15.1",
    "libp2p-webrtc-star": "^0.20.5",
    "libp2p-websockets": "^0.15.0",
    "multiaddr": "^8.1.2",
    "multiformats": "^4.4.1",
    "peer-id": "^0.14.2"
  }
}
